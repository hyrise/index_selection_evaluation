{
    "database_system": "postgres",
    "benchmark_name": "tpch",
    "scale_factor": 0.1,
    "pickle_workload": true,
    "algorithms": [
        {
            "name": "microsoft",
            "parameters": {
                "max_indexes": 10,
                "max_indexes_naive": 2,
